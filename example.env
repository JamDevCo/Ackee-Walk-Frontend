# Laravel API URL
NEXT_PUBLIC_API_URL=http://localhost:8000/api

# If you're using authentication, you might need these
NEXT_PUBLIC_AUTH_TOKEN_KEY=your_auth_token_key
NEXT_PUBLIC_AUTH_USER_KEY=your_auth_user_key

# If you're using a specific version of the API
NEXT_PUBLIC_API_VERSION=v1

# Any other environment-specific variables
NEXT_PUBLIC_APP_ENV=development

# If you're using CORS and need to specify allowed origins
NEXT_PUBLIC_CORS_ALLOWED_ORIGINS=http://localhost:3000

# If you need to specify a timeout for API requests
NEXT_PUBLIC_API_TIMEOUT=30000